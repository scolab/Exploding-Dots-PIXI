{
  "name": "@scolab/component-exploding-dots",
  "version": "0.3.5",
  "description": "Exploding dots component",
  "engines": {
    "node": ">=8.6.0",
    "npm": "5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/scolab/Exploding-Dots-PIXI.git"
  },
  "license": "MIT",
  "scripts": {
    "lint": "eslint src",
    "lintfix": "eslint src --fix",
    "testonly": "mocha --require .scripts/mocha_runner src/**/tests/**/*.js",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js",
    "storybook": "start-storybook -s ./static -p 9010",
    "publish-storybook": "bash .scripts/publish_storybook.sh",
    "prepublishOnly": "bash .scripts/prepublish.sh",
    "build": "rimraf ./dist/ && npm run .build-index.d.ts && npm run .build-dist",
    ".build-index.d.ts": "tsc && npm run .build-index.d.ts-clear",
    ".build-index.d.ts-clear": "node .scripts/cleandist.js",
    ".build-dist": "set NODE_ENV=production && webpack --config webpack.dist.config.js"
  },
  "devDependencies": {
    "@storybook/addons": "^3.3.15",
    "@storybook/react": "^3.3.15",
    "@types/lodash": "^4.14.70",
    "@types/node": "^7.0.22",
    "@types/pixi.js": "4.5.1",
    "@types/react": "16.0.36",
    "@types/react-dom": "16.0.3",
    "@types/victor": "^1.1.0",
    "awesome-typescript-loader": "^3.1.3",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-inline-environment-variables": "^0.3.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "^0.2.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^3.5.0",
    "enzyme": "^2.2.0",
    "eslint": "^3.6.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "git-url-parse": "^6.0.1",
    "jsdom": "^9.5.0",
    "mocha": "^3.0.2",
    "prop-types": "^15.5.10",
    "react": "16.2.0",
    "react-addons-test-utils": "^15.3.2",
    "react-dom": "16.2.0",
    "react-storybook-decorator-background": "^1.0.5",
    "shelljs": "^0.8.2",
    "sinon": "^1.17.6",
    "source-map-loader": "^0.2.1",
    "styled-components": "3.1.6",
    "ts-loader": "^2.2.2",
    "tslint": "5.5.0",
    "tslint-config-airbnb": "^5.7.0",
    "typescript": "2.4.1",
    "webpack-bundle-analyzer": "^2.11.1"
  },
  "peerDependencies": {
    "react": "^16.2.0",
    "styled-components": "^3.1.6"
  },
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "detect-browser": "^2.0.0",
    "eventemitter3": "^2.0.2",
    "express": "^4.14.1",
    "gsap": "^1.20.2",
    "howler": "^2.0.3",
    "katex": "^0.8.3",
    "lodash": "^4.17.4",
    "pixi-particles": "^2.1.4",
    "pixi.js": "4.5.1",
    "react-fastclick": "^3.0.2",
    "react-redux": "^5.0.1",
    "react-visibility-sensor": "^3.10.0",
    "redux": "^3.6.0",
    "victor": "^1.1.0"
  },
  "main": "dist/containers/App.server.js",
  "browser": "dist/containers/App.client.js"
